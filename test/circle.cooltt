def path (A : type) (a b : A) : type :=
  ext i => A with [i=0 => a | i=1 => b]

def Ω1s1 : type :=
  path circle base base

def loopn : nat -> Ω1s1 :=
  elim [
  | zero => _ => base
  | suc {n => loopn} =>
    i =>
    hcom circle 0 1 {∂ i} {k _ =>
      [ k=0 => loopn i
      | i=0 => base
      | i=1 => loop k
      ]
    }
  ]
