def + : nat â†’ nat â†’ nat = {
  Î» elim [
  | zero => n => n
  | suc {_ => ih} => n => suc {ih n}
  ]
}

def +-right-unit : (x : nat) â†’ path {_ => nat} {+ x 0} x = {
  Î» elim [
  | zero =>
    _ => 0
  | suc {y => ih} =>
    i => suc {ih i}
  ]
}

def +-left-unit : (x : nat) â†’ path {_ => nat} {+ 0 x} x = {
  x _ => x
}

def +-suc-r : (x : nat) (y : nat) â†’ path {_ => nat} {+ x {suc y}} {suc {+ x y}} = {
  Î» elim [
  | zero =>
    x i => {suc x}
  | suc {x => ih} =>
    y i => suc {ih y i}
  ]
}

def +-comm : (x : nat) (y : nat) â†’ path {_ => nat} {+ y x} {+ x y} = {
  Î» elim [
  | zero => +-right-unit
  | suc {y => ih} =>
    z =>
    trans nat {+-suc-r z y} {j => suc {ih z j}}
  ]
}

def +-assoc : {
  (x : nat) (y : nat) (z : nat) â†’
  path {_ => nat} {+ {+ x y} z} {+ x {+ y z}}
} = {
  Î» elim [
  | zero => y z i => + y z
  | suc {x => ih} => y z i => suc {ih y z i}
  ]
}

def test : {
  (p : ğ•€ â†’ nat) â†’ (i : ğ•€) â†’ nat
} = {
  p i =>
  rec {symm nat p i} @ nat [
  | zero => zero
  | suc {x => ih} => zero
