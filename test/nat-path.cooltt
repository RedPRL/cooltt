def symm : {
  (A : univ) (p : ð•€ â†’ A) â†’ path {\_ => A} {p 1} {p 0}
}= {
  \A p i =>
  hcom A 0 1 {\j _ =>
    [ j=0 \/ i=1 => p 0
    | i=0 => p j
    ]
  }
}

def trans/filler : {
  (A : univ) (p : ð•€ â†’ A) (q : (i : ð•€) â†’ sub A {i=0} {p 1}) â†’ ð•€ â†’ ð•€ â†’ A
} = {
  \A p q j i =>
  hcom A 0 j {âˆ‚ i} {\j _ =>
    [ j=0 âˆ¨ i=0 => p i
    | i=1 => q j
    ]
  }
}

def trans : {
  (A : univ) (p : ð•€ â†’ A) (q : (i : ð•€) â†’ sub A {i=0} {p 1}) â†’ path {\_ => A} {p 0} {q 1}
} = {
  Î» A p q i => trans/filler A p q 1 i
}


def J : {
  (A : univ) (p : ð•€ â†’ A) (C : {(i : ð•€) â†’ sub A {i=0} {p 0}} â†’ univ) â†’
  {C {Î» _ => p 0}} â†’ C p
} = {
  Î» A p C d =>
  coe {Î» i =>
    C {Î» j =>
      hcom A 0 j {âˆ‚ i} {\k _ => [k=0 âˆ¨ i=0 => p 0 | i=1 => p k]}
    }
  } 0 1 d
}

