def symm : {
  (A : type) (p : ğ•€ â†’ A) â†’ path {_ => A} {p 1} {p 0}
}= {
  A p i =>
  hcom A 0 1 {j _ =>
    [ j=0 âˆ¨ i=1 => p 0
    | i=0 => p j
    ]
  }
}

def trans/filler : {
  (A : type) (p : ğ•€ â†’ A) (q : (i : ğ•€) â†’ sub A {i=0} {p 1}) â†’ ğ•€ â†’ ğ•€ â†’ A
} = {
  A p q j i =>
  hcom A 0 j {âˆ‚ i} {j _ =>
    [ j=0 âˆ¨ i=0 => p i
    | i=1 => q j
