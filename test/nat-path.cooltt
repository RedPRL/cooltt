  }
}

def trans : {
  (A : type) (p : ğ•€ â†’ A) (q : (i : ğ•€) â†’ sub A {i=0} {p 1}) â†’ path {_ => A} {p 0} {q 1}
} = {
  A p q => trans/filler A p q 1
}


def J : {
  (A : type) (p : ğ•€ â†’ A) (C : {(i : ğ•€) â†’ sub A {i=0} {p 0}} â†’ type) â†’
  {C {_ => p 0}} â†’ C p
} = {
  A p C d =>
  coe {i => C {hfill A 0 {âˆ‚ i} {k _ => [k=0 âˆ¨ i=0 => p 0 | i=1 => p k]}}} 0 1 d
}

normalize J

