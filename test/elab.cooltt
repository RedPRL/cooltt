def boundary-test : (i : dim) (_ : [{i==0} \/ {i==1}]) -> nat = {
  \i _ =>
   [ i==1 => 5
   | i==0 => 19
   ]
}

def subtype-test : (i : dim) -> sub nat {{i==0} \/ {i==1}} [i==0 => 4 | i==1 => 5] = {
  \i => [i==0 => 4 | i==1 => 5]
}

normalize subtype-test
normalize boundary-test

def reflexivity : (A : univ) (a : A) (i : dim) -> A = {
  \A a _ => a
}

def foo : (x : nat) -> (y : nat) * Id nat x y = {
  \x =>
  [x, ?hole1]
}

def sym : (A : univ) (x : A) (y : A) (p : Id A x y) -> Id A y x = {
  \A x y p =>
  elim p @ {u v _ => Id A v u} [
  | refl => refl
  ]
}

def trans : {
  (A : univ) (x : A) (y : A) (z : A)
  (p : Id A x y) (q : Id A y z)
  -> Id A x z
} = {
  \A x y z p =>
  elim p @ {u v _ => (q : Id A v z) -> Id A u z} [
  | refl => \q => q
  ]
}

normalize sym
normalize trans

def hole-in-type : {
  (x : nat) (y : nat) (z : nat)
  (p : Id nat x y) (q : Id nat y z)
  -> ?tyhole
} = {
  \y z => ?tmhole
}


def pi-code-test : univ = (x : nat) -> nat
def id-code-test : univ = (x : nat) * Id nat x x

normalize pi-code-test
normalize id-code-test
