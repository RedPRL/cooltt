def v-test (r : 𝕀) (A : type) : type =
  V r {_ => A} A {_ =>
    [ x => x
    , x =>
      [ [x, _ => x]
      , p i =>
        let aux = hfill A 1 {∂ i} {k _ => [ k=1 => x | i=1 => {snd p} k | i=0 => x ] } in
        [aux 0, aux]
      ]
    ]
  }

normalize v-test

def scary (A : type) (a : A) : A =
  coe {i => v-test i A} 0 1 a
