def v-test (r : ð•€) (A : type) : type :=
  V r {_ => A} A {_ =>
    [ x => x
    , x =>
      [ [x, _ => x]
      , p i =>
        let aux := hfill A 1 {âˆ‚ i} {k _ => [ k=1 => x | i=1 => {snd p} k | i=0 => x ] } in
        [aux 0, aux]
