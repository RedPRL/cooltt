  }

#normalize v-test

def cool (A : type) (a : A) : sub A âŠ¤ {coe {_ => A} 0 1 a} :=
  coe {i => v-test i A} 0 1 a

def cool2 (A : type) (a : A) (i : ð•€) : A :=
  coe {i => v-test i A} i 0 [_ => a, a]

#normalize cool
#normalize cool2

def cool3 (A : type) (a : A) (i : ð•€) : sub A âŠ¤ a :=
  let vin : v-test i A := [_ => a, a] in
