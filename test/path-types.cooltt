def funext : {
  (A : type) (B : type) (f : (x : A) â†’ B) (g : (x : A) â†’ B)
  (h : (x : A) â†’ pathd {_ => B} {f x} {g x})
  â†’ pathd {_ => (x : A) â†’ B} f g
} = {
  A B f g h i x =>
  h x i
}

normalize funext

def pairext : {
  (A : type) (B : type) (p : (x : A) Ã— B) (q : (x : A) Ã— B)
  (h : (x : pathd {_ => A} {fst p} {fst q}) Ã— pathd {_ => B} {snd p} {snd q})
  â†’ pathd {_ => (x : A) Ã— B} p q
} = {
  A B p q h i =>
  [ {fst h} i
  , {snd h} i
  ]
}

normalize pairext
