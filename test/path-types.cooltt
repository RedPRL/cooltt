def formation : {
  (A : ð•€ â†’ type) (a : A 0) (b : A 1) â†’ type
} := {
  A a b =>
  ext i => A i with [ i=0 => a | i=1 => b]
}

normalize formation

def path (A : type) (a : A) (b : A) : type :=
  ext i => A with [i=0 => a | i=1 => b]


def myrefl : {
  (A : type) (a : A) â†’ path A a a
} := {
  A a i => a
}


normalize myrefl

def funext : {
  (A : type) (B : type) (f : (x : A) â†’ B) (g : (x : A) â†’ B)
  (h : (x : A) â†’ path B {f x} {g x})
  â†’ path {(x : A) â†’ B} f g
} := {
  A B f g h i x =>
  h x i
}

normalize funext

def pairext : {
  (A : type) (B : type) (p : (x : A) Ã— B) (q : (x : A) Ã— B)
  (h : (x : path A {fst p} {fst q}) Ã— path B {snd p} {snd q})
  â†’ path {(x : A) Ã— B} p q
} := {
  A B p q h i =>
  [ {fst h} i
  , {snd h} i
  ]
}

normalize pairext
