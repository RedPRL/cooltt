#normalize formation

def path (A : type) (a : A) (b : A) : type :=
  ext i => A with [i=0 => a | i=1 => b]


def myrefl : {
  (A : type) (a : A) â†’ path A a a
} := {
  A a i => a
}


#normalize myrefl

def funext : {
  (A B : type) (f : (x : A) â†’ B) (g : (x : A) â†’ B)
  (h : (x : A) â†’ path B {f x} {g x})
  â†’ path {(x : A) â†’ B} f g
} := {
  A B f g h i x =>
  h x i
}

#normalize funext

def funextdep : {
  (A : ð•€ â†’ type) (B : (i : ð•€) â†’ A i â†’ type)
  (f : (i : ð•€) â†’ [âˆ‚ i] â†’ (x : A i) â†’ B i x)
  (h : (p : (i : ð•€) â†’ A i) â†’ ext i => B i {p i} with [âˆ‚ i => f i {p i}])
  â†’ ext i => (x : A i) â†’ B i x with [âˆ‚ i => f i]
} := {
  A B f h i x =>
  h {j => coe A i j x} i
}

def pairext : {
  (A B : type) (p : (x : A) Ã— B) (q : (x : A) Ã— B)
  (h : (x : path A {fst p} {fst q}) Ã— path B {snd p} {snd q})
  â†’ path {(x : A) Ã— B} p q
} := {
  A B p q h i =>
  [ {fst h} i
  , {snd h} i
  ]
}

#normalize pairext
