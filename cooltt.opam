opam-version: "2.0"
name: "cooltt"
version: "0.0"
maintainer: "Daniel Gratzer <gratzer@cs.au.dk>"
authors: ["Daniel Gratzer" "Jonathan Sterling"]
homepage: "https://github.com/jozefg/cooltt"
